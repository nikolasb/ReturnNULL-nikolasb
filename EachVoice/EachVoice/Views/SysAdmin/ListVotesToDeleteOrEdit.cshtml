@model IEnumerable<EachVoice.Models.CustomU.UserVote>

@{
    ViewBag.Title = "ListVotesToDeleteOrEdit";
}

<h2>ListVotesToDeleteOrEdit</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.votebit)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>@Html.DisplayFor(modelItem => item.id)</td>
        <td>
            @Html.DisplayFor(modelItem => item.votebit)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ucblid)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ucnetuid)
        </td>
        <td>            
            @Html.ActionLink("Delete", "DeleteVote", new {id = item.id})
        </td>
    </tr>
}

</table>
