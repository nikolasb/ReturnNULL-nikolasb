
@{
    ViewBag.Title = "SearchToDeleteOrEdit";
}

<h2>SearchToDelete</h2>
<p class="text-success">@ViewBag.Message</p>
<div class="panel panel-default">
    <div class="panel-body">
        <form class="form-inline" role="form" id="userform" action="ListUsersToDeleteOrEdit" controller="SysAdmin" method="get">
            <div class="input-group">


                <button type="submit" class="btn btn-default">
                    <span class="glyphicon glyphicon-search" aria-hidden="true"></span>
                    Search Users
                </button>

                <div class="form-group">
                    <input type="text" id="phoneNumber" name="phoneNumber" size="500" class="form-control" placeholder="Phone#...">
                </div>
                <div class="form-group">
                    <input type="text" id="userName" name="userName" size="500" class="form-control" placeholder="User Name...">
                </div>

            </div>
        </form>
    </div>
</div>

<div class="panel panel-default">
    <div class="panel-body">
        <form class="form-inline" role="form" id="commentform" action="ListCommentsToDeleteOrEdit" controller="SysAdmin" method="get">
            <div class="input-group">


                <button type="submit" class="btn btn-default">
                    <span class="glyphicon glyphicon-search" aria-hidden="true"></span>
                    Search Comments
                </button>

                <div class="form-group">
                    <input type="text" id="bill" name="blid" size="500" class="form-control" placeholder="Bill id#...">
                </div>
            </div>
        </form>
    </div>
</div>

<div class="panel panel-default">
    <div class="panel-body">
        <form class="form-inline" role="form" id="voteform" action="ListVotesToDeleteOrEdit" controller="SysAdmin" method="get">
            <div class="input-group">


                <button type="submit" class="btn btn-default">
                    <span class="glyphicon glyphicon-search" aria-hidden="true"></span>
                    Search Votes
                </button>

                <div class="form-group">
                    <input type="text" id="uid" name="uid" size="500" class="form-control" placeholder="User Id...">
                </div>
                <div class="form-group">
                    <input type="text" id="blid" name="blid" size="500" class="form-control" placeholder="Bill Id...">
                </div>

            </div>
        </form>
    </div>
</div>


